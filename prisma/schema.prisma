generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(uuid())
  username String @unique
  email String @unique
  password String
  createdAt DateTime @default(now())
}

model Tournament {
  id String @id @default(uuid())
  name String
  status String
  createdAt DateTime @default(now())

}